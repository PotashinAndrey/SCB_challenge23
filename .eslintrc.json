{
    "root": true,
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:import/recommended",
        "plugin:react-hooks/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "react",
        "effector",
        "@typescript-eslint",
        "prettier"
    ],
    "ignorePatterns": ["src/**/*.test.ts", "src/frontend/generated/*"]
}
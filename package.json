    "frontend-build": "cd ./source/frontend && webpack build --config ./webpack.config.ts",
    "clear": "npm run backend-clear && rm -rf package-lock.json node_modules/",
    "prettier": "prettier --config .prettierrc \"**/*.{ts,tsx}\" --write",
